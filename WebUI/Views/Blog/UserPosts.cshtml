@model IEnumerable<WebUI.Models.BlogModels.PostModel>

@{
    ViewBag.Title = "UserPosts";
}

<div class="blog">
    <h2>Your Posts</h2>
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-md-10 mx-auto">
                @foreach (var post in Model)
                {
                    @Html.Partial("_PostPreview", post)
                    <hr />
                }
            </div>
        </div>
    </div>
</div>
